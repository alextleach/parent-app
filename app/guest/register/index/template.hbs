{{outlet}}
<div class="container">
  <div class="card-container">
    <div class="row">
      <div class="card-card__title col s12 center-align">Register Below</div>
    </div>

    <div class="row">
      <form class="col s12">
        <div class="row">
          <div class="input-field col s12">
            <input id="email" type="email" class="validate">
            <label for="email">Email</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="password" type="password" class="validate">
            <label for="password">Password</label>
          </div>
        </div>
        <div class="form-action center-align">
          <button class="btn col s12 card-btn btn-large"><i class="fa fa-arrow-right"></i></button>
          <button class="btn back-btn">Already have an account?</button>
        </div>
      </form>
    </div>
  </div>
</div>
